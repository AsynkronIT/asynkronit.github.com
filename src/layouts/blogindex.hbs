---
layout: master.hbs
active_selector: blog
---
<div role="main" class="main">
	<section class="page-header">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul class="breadcrumb">
						<li><a href="/index.html">Home</a></li>
						<li><a href="/blog/index.html">Blog</a></li>
						<li class="active">{{basename}}</li>
					</ul>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<h1>Blog</h1>
				</div>
			</div>
		</div>
	</section>

	<div class="container">
		<div class="row">
			<div class="col-md-3">
				<aside class="sidebar">

					<h4 class="heading-primary">Topics</h4>
					<div id="toc">
						<ul>
  {{#withSort post 'data.posted'}}
    {{#each this.pages }}
<li><a href="{{ replaceStr this.dest 'web' '' }}">{{ this.data.title }}</a></li>
				    
    {{/each}}
  {{/withSort}}
</ul>
					</div>
				</aside>
			</div>
			<div class="col-md-9" id="main_content">
				
			</div>

		</div>
	</div>
</div>