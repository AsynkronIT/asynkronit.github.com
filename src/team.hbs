---
layout: master.hbs
title: Team
active_selector: team
---

<div role="main" class="main">
	<section class="page-header">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul class="breadcrumb">
						<li><a href="/index.html">Home</a></li>
						<li class="active">Team</li>
					</ul>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<h1>The Team</h1>
				</div>
			</div>
		</div>
	</section>
    <div class="container">

        <ul class="nav nav-pills sort-source" data-sort-id="team" data-option-key="filter">
            <li data-option-value="*" class="active"><a href="#">Show All</a></li>
            <li data-option-value=".dotnet"><a href="#">.NET</a></li>
            <li data-option-value=".go"><a href="#">Go</a></li>
        </ul>

        <hr>

        <div class="row">

            <div class="sort-destination-loader sort-destination-loader-showing">
                <ul class="team-list sort-destination" data-sort-id="team">

                    {{#each team}}
                    
                    <li class="col-md-3 col-sm-6 col-xs-12 isotope-item {{this.platforms}}">
                        <span class="thumb-info thumb-info-hide-wrapper-bg mb-xlg">
                            <span class="thumb-info-wrapper">
                                <img src="{{this.profile_image}}" class="img-responsive" alt="">
                                <span class="thumb-info-title">
                                    <span class="thumb-info-inner">{{this.name}}</span>
                                    <span class="thumb-info-type">{{this.title}}</span>
                                </span>
                            </span>
                            <span class="thumb-info-caption">
                                <span class="thumb-info-caption-text">{{this.description}}</span>
                                <span class="thumb-info-social-icons">
                                    <a href="{{this.twitter}}"><i class="fa fa-twitter"></i><span>Twitter</span></a>
                                    <a href="{{this.github}}"><i class="fa fa-github"></i><span>Github</span></a>
                                </span>
                            </span>
                        </span>
                    </li>
                    {{/each}}
                    
                </ul>
            </div>

        </div>
    </div>
</div>